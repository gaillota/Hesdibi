{% extends 'AGVaultBundle::layout.html.twig' %}

{% block breadcrumbs %}
	{{ parent() }}
	{% for parent in listParents %}
		<li><a href="{{ path('ag_vault_folder_show', { id:parent.id }) }}">{{ parent.name }}</a></li>
	{% endfor %}
	<li class="active">Nouveau</li>
{% endblock %}

{% block ag_vault_content %}
	<div class="row">
		<div class="col-md-12">
			<h2>{{ fa('plus') }} Nouveau dossier</h2>
			<hr/>
			{% include 'AGVaultBundle:Folder:form.html.twig' %}
		</div>
	</div>
{% endblock %}